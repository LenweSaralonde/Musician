<Bindings>
	<Binding name="MUSICIANTOGGLE" category="BINDING_CATEGORY_MUSICIAN">
		MusicianFrame:Toggle()
	</Binding>
	<Binding name="MUSICIANPLAY" category="BINDING_CATEGORY_MUSICIAN">
		Musician.Comm.TogglePlaySong()
	</Binding>
	<Binding name="MUSICIANKEYBOARD" category="BINDING_CATEGORY_MUSICIAN">
		MusicianFrameSource:ClearFocus()
		Musician.Keyboard.Show()
	</Binding>
</Bindings>