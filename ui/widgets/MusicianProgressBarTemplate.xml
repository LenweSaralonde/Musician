<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.blizzard.com/wow/ui/">
	<!-- Progress bar -->
	<Script file="MusicianProgressBarTemplate.lua" />
	<Frame name="MusicianProgressBarTemplate" mixin="MusicianProgressBarTemplateMixin" virtual="true">
		<Size>
			<AbsDimension y="10"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBackground" setAllPoints="true">
					<Color a="1" r="0" g="0" b="0" />
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture name="$parentFill" parentKey="fill">
					<Anchors>
						<Anchor point="TOP" />
						<Anchor point="LEFT" />
						<Anchor point="BOTTOM" />
					</Anchors>
					<Size x="0" />
					<Color a="1" r="1" g="1" b="1" />
				</Texture>
			</Layer>
		</Layers>
	</Frame>
</Ui>